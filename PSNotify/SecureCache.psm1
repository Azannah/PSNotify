function Protect-Data {
  [CmdletBinding()]
  Param(

    [Parameter(
      HelpMessage="Provide a Protector object",
      Mandatory=$true,
      #ParameterSetName="protector",
      #Position=0,
      ValueFromPipeline=$false,
      ValueFromPipelineByPropertyName=$true,
      ValueFromRemainingArguments=$false)]
    #[Alias('Password','secret')]
    [object]
    # A 'Protector' object, which specifies encryption options and manages the encryption keys (use New-Protector)
    $Protector,

    [Parameter(
      HelpMessage="Specify the data to be protected",
      Mandatory=$true,
      ParameterSetName="protector",
      #Position=0,
      ValueFromPipeline=$false,
      ValueFromPipelineByPropertyName=$true,
      ValueFromRemainingArguments=$false)]
    [Alias('UnprotectedData','Data')]
    [object]
    # The data to be protected must be serializable
    $InputObject,

    [Parameter(
      #HelpMessage="Provide a Protector object",
      Mandatory=$false,
      ParameterSetName="protector",
      #Position=0,
      ValueFromPipeline=$false,
      ValueFromPipelineByPropertyName=$true,
      ValueFromRemainingArguments=$false)]
    #[Alias('Password','secret')]
    [switch]
    # If this switch is not specified, the initialization vector (IV) is embedded with the protected data, which only the Protect-Data and Unprotect-Data cmdlets understand.
    $DontEmbeddIV = $false,

    [Parameter(
      HelpMessage="Speciy the output encoding",
      Mandatory=$false,
      ParameterSetName="protector",
      #Position=0,
      ValueFromPipeline=$false,
      ValueFromPipelineByPropertyName=$true,
      ValueFromRemainingArguments=$false)]
    #[Alias('Password','secret')]
    [ValidateSet('Base64String','Bytes')]
    [string]
    # Specifies how the returned value should be encoded
    $OutputEncoding = 'Bytes'
  )

  Begin {

    function private:encrypt_using_aesmanaged {

      Param (
        [byte[]]$data_to_encrypt,
        [byte[]]$iv,
        [byte[]]$key
      )

      [System.Security.Cryptography.AesManaged] $aes_managed = New-Object System.Security.Cryptography.AesManaged
      $aes_managed.Key = $key
      $aes_managed.IV = $iv
      $aes_managed.Padding = [System.Security.Cryptography.PaddingMode]::PKCS7

      $encryptor = $aes_managed.CreateEncryptor()

      $memory_stream = New-Object System.IO.MemoryStream

      $crypto_stream = New-Object System.Security.Cryptography.CryptoStream $memory_stream, $encryptor, ([System.Security.Cryptography.CryptoStreamMode]::Write)
      $crypto_stream.Write(([byte[]] $data_to_encrypt), 0, $data_to_encrypt.Length)

      <#
      Reference: https://www.simple-talk.com/blogs/2012/02/28/oh-no-my-paddings-invalid/
      CryptoStream has a special method to flush the final block of data  FlushFinalBlock. Calling Stream.Flush() does not flush the 
      final block, as you might expect. Only by closing the stream or explicitly calling FlushFinalBlock is the final block, with any 
      padding, encrypted and written to the backing stream. Without this call, the encrypted data is 16 bytes shorter than it should be.

      Hence, $crypto_stream must be disposed (and closed) before the $memory_stream can be converted to an array.
      #>
      $crypto_stream.Dispose()

      $encrypted_data = $memory_stream.ToArray()

      $memory_stream.Dispose()

      return $encrypted_data

      <#
        try
        {
            $secureString = 'This is my password.  There are many like it, but this one is mine.' | 
                            ConvertTo-SecureString -AsPlainText -Force

            # Generate our new 32-byte AES key.  I don't recommend using Get-Random for this; the System.Security.Cryptography namespace
            # offers a much more secure random number generator.

            $key = New-Object byte[](32)
            $rng = [System.Security.Cryptography.RNGCryptoServiceProvider]::Create()

            $rng.GetBytes($key)

            $encryptedString = ConvertFrom-SecureString -SecureString $secureString -Key $key

            # This is the thumbprint of a certificate on my test system where I have the private key installed.

            $thumbprint = 'B210C54BF75E201BA77A55A0A023B3AE12CD26FA'
            $cert = Get-Item -Path Cert:\CurrentUser\My\$thumbprint -ErrorAction Stop

            $encryptedKey = $cert.PublicKey.Key.Encrypt($key, $true)

            $object = New-Object psobject -Property @{
                Key = $encryptedKey
                Payload = $encryptedString
            }

            $object | Export-Clixml .\encryptionTest.xml

        }
        finally
        {
            if ($null -ne $key) { [array]::Clear($key, 0, $key.Length) }
        }
      #>
    }

    function private:encrypt_using_dpapi {

      Param (
        [byte[]]$data_to_encrypt,
        [byte[]]$iv,
        [System.Security.Cryptography.DataProtectionScope]$scope
      )

      [byte[]]$encrypted_data = [System.Security.Cryptography.ProtectedData]::Protect($data_to_encrypt, $iv, $scope)

      return $encrypted_data

    }

    function private:embed_nonce_in_byte_array {
      Param (
        [Byte[]]$nonce,
        [Byte[]]$data
      )

      [Byte[]]$preamble = @(170,170,170,171)
      [Byte[]]$nonce_size_32bits = [System.BitConverter]::GetBytes([Int32]$nonce.Count)

      ## Make sure the size is stored using Big Endian
      if ([System.BitConverter]::IsLittleEndian) {
        [array]::Reverse($nonce_size_32bits)
      }

      return ($preamble + $nonce_size_32bits + $nonce + $data)
    }

  }
  
  Process {

    ## Attempt to serialize the object, and fail
    try {
      $serialized_object = ConvertTo-SerializedObject -InputObject $InputObject
    } catch {
      # 2do: log
      throw [System.Security.Cryptography.CryptographicException] "The object of type $($InputObject.GetType().Name) cannot be serialized for encryption"
    }

    ## Get the randomly generated nonce we're going to use for encryption from the Protector
    [Byte[]]$nonce = $Protector.GetNonce()

    ## Encrypt the serialized object using the specified protector type
    [Byte[]]$encrypted_serialized_object

    switch ($Protector.GetProtectorType()) {
      {$_ -match "DPAPI"} {
        try {
          # 2do: The DPAPI scope should be defined on the Protector
          $encrypted_serialized_object = private:encrypt_using_dpapi -data_to_encrypt $serialized_object -iv $nonce -scope CurrentUser
        } catch {
          # 2do: log
          throw $_
        }

        break
      }

      {$_ -match "AES"} {
        try {
          $encrypted_serialized_object = private:encrypt_using_aesmanaged -data_to_encrypt $serialized_object -iv $nonce -key ($Protector.GetSymmetricKey())
        } catch {
          # 2do: log
          throw $_
        }

        break
      }
    }

    if (-not $DontEmbeddIV) {
      $encrypted_serialized_object = private:embed_nonce_in_byte_array -nonce $nonce -data $encrypted_serialized_object
    }

    switch ($OutputEncoding) {
      {$_ -match "Base64String"} {
        $encrypted_base64_object = [System.Convert]::ToBase64String($encrypted_serialized_object)
        return $encrypted_base64_object
      }

      {$_ -match "Bytes"} {
        return $encrypted_serialized_object
      }
    }

  }

  End {  }
}

function ConvertTo-SerializedObject {
  Param (
    [object]$InputObject
  )

  ## Fist-thing-first, can we even serialized this object so it can be protected in the first place?
  if (-not $InputObject.GetType().IsSerializable) {
    throw [System.Runtime.Serialization.SerializationException] "The object of type $($InputObject.GetType().Name) provided is not serializable"
  }

  ## Create a binary formatter and a MemoryStream to receive its output
  $memory_stream = New-Object System.IO.MemoryStream
  $binary_formatter = New-Object System.Runtime.Serialization.Formatters.Binary.BinaryFormatter

  ## Serialize the object. Will throw an exception here if the process fails
  try {
    $binary_formatter.Serialize($memory_stream, $InputObject)
    $serialized_object = $memory_stream.ToArray()
  } finally {
    $memory_stream.Close()
  }

  return $serialized_object
}


